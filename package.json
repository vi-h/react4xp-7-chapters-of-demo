{
    "name": "starter-react4xp",
    "version": "2.1.0",
    "description": "React4xp starter for Enonic XP7.0",
    "scripts": {
        "browserSync": "npx browser-sync start --proxy 'localhost:8080' --files build/resources/main/**/* --reload-delay 5000",
        "build": "./gradlew build",
        "clean": "rm -r build 2>/dev/null; true",
        "deploy": "./gradlew deploy",
        "deps": "./gradlew --refresh-dependencies",
        "rewatch": "./gradlew clean deploy && ./gradlew -t deploy",
        "watch": "./gradlew -t deploy",
        "postinstall": "node monkeypatch.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enonic/starter-react4xp.git"
    },
    "author": "Espen Norderud",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/enonic/starter-react4xp/issues"
    },
    "homepage": "https://github.com/enonic/starter-react4xp#readme",
    "dependencies": {
        "@emotion/react": "11.9.0",
        "@emotion/styled": "11.8.1",
        "@mui/icons-material": "5.8.0",
        "@mui/material": "5.8.1",
        "@enonic/js-utils": "^0.38.1",
        "@enonic/react-components": "^3.0.1",
        "node-fetch": "^3.2.4",
        "prop-types": "15.8.1",
        "react": "18.0.0",
        "react-app-polyfill": "3.0.0",
        "react-dom": "18.1.0"
    },
    "devDependencies": {
        "@babel/cli": "7.17.10",
        "@babel/compat-data": "7.17.10",
        "@babel/core": "7.18.2",
        "@babel/plugin-proposal-class-properties": "7.17.12",
        "@babel/plugin-proposal-object-rest-spread": "7.18.0",
        "@babel/plugin-transform-arrow-functions": "7.16.7",
        "@babel/preset-env": "7.18.2",
        "@babel/preset-react": "7.17.12",
        "@enonic/react4xp": "^3.0.18",
        "babel": "^6.23.0",
        "babel-loader": "8.2.5",
        "browser-sync": "^2.27.10",
        "css-loader": "6.7.1",
        "core-js": "3.22.7",
        "cssnano": "5.1.10",
        "file-loader": "6.2.0",
        "npm-run-all": "4.1.5",
        "mini-css-extract-plugin": "2.6.0",
        "postcss": "8.4.14",
        "postcss-csso": "6.0.0",
        "postcss-critical-split": "2.5.3",
        "postcss-pipeline-webpack-plugin": "6.0.0",
        "sass": "1.52.1",
        "sass-loader": "13.0.0",
        "stylelint": "14.8.5",
        "stylelint-webpack-plugin": "3.3.0",
        "webpack": "5.72.1",
        "webpack-cli": "4.9.2"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-object-rest-spread",
            "@babel/plugin-transform-arrow-functions"
        ]
    }
}
